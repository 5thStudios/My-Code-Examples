@using ContentModels = Umbraco.Web.PublishedModels;
@inherits UmbracoViewPage<ContentModels.BlockList>
@{ Layout = "TopLevel.cshtml";}



<div class="blockList">
    <div class="grid-x small-up-1 medium-up-2 large-up-4">
        @foreach (IPublishedContent ipPage in Model.Children.Where(x => x.IsVisible()))
        {
                @Html.Partial("Partials/Generic/BlockItem", ipPage)
        }
    </div>
</div>